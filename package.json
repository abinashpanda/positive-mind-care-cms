{
  "name": "positive-mind-care-cms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --noEmit --incremental",
    "check-types:watch": "tsc --noEmit --incremental --watch",
    "seed:tests": "tsup ./src/seed-collections/tests/tests.ts --onSuccess \"node ./dist/tests.js\"",
    "seed:services": "tsup ./src/seed-collections/service/services.ts --onSuccess \"node ./dist/services.js\"",
    "seed:messages": "tsup ./src/seed-collections/message/messages.ts --onSuccess \"node ./dist/messages.js\"",
    "seed:questions": "tsup ./src/seed-collections/questions/questions.ts --onSuccess \"node ./dist/questions.js\"",
    "seed:score": "tsup ./src/seed-collections/score/score.ts --onSuccess \"node ./dist/score.js\"",
    "seed-data": "concurrently \"npm run seed:*\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/lab": "^5.0.0-alpha.140",
    "@mui/material": "^5.14.5",
    "@mui/x-date-pickers": "^5.0.20",
    "@t3-oss/env-nextjs": "^0.6.1",
    "@types/node": "20.5.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "cz-conventional-changelog": "^3.3.0",
    "firebase": "^9.23.0",
    "firebase-admin": "^11.10.1",
    "firecms": "^2.0.5",
    "next": "13.4.16",
    "postcss": "8.4.28",
    "razorpay": "^2.9.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "resend": "^2.0.0",
    "tailwindcss": "3.3.3",
    "ts-pattern": "^5.0.6",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.16",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "chalk": "^5.3.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.47.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
